{
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "semi": true,
  "tabWidth": 2,
  "trailingComma": "none",
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "<BUILTIN_MODULES>",
    "^react",
    "<THIRD_PARTY_MODULES>",
    "^@inkdropapp",
    "^@[/]",
    "^[.].*$",
    "<TYPES>",
    "^[/].*$",
    "[.]css$"
  ]
}
